	"description": "Useful Twig extensions for your Symfony2 project.",
	"keywords": ["symfony2", "twig", "extensions"],
	"homepage": "https://github.com/craue/TwigExtensionsBundle",
	"license": "MIT",
	"authors": [
		{
			"name": "Christian Raue",
			"email": "christian.raue@gmail.com"
		},
		{
			"name": "Symfony Community",
			"homepage": "https://github.com/craue/TwigExtensionsBundle/contributors"
		}
	],
	"require": {
		"php": ">=5.3.2",
		"symfony/twig-bundle": ">=2.0-dev,<2.2-dev"
	},
	"require-dev": {
		"symfony/framework-bundle": ">=2.0-dev,<2.2-dev",
		"symfony/finder": ">=2.0-dev,<2.2-dev"
	},
	"autoload": {
		"psr-0": {
			"Craue\\TwigExtensionsBundle": ""
		}
	},
	"target-dir": "Craue/TwigExtensionsBundle"
}
